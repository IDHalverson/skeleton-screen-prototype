{
  "name": "skeleton-view-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "do-skeleton-view-script": "node ./scripts/insert-skeleton-view.js"
  },
  "watch": {
    "do-skeleton-view-script": {
      "patterns": [
        "public/skeleton-view/constants.js",
        "public/skeleton-view/skeleton-view.css",
        "public/skeleton-view/skeleton-view.js",
        "public/skeleton-view/skeleton-view.html"
      ],
      "quiet": false,
      "runOnChangeOnly": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "fs-extra": "^10.1.0",
    "uglify-js": "^3.15.5"
  }
